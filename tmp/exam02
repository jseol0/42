union	-- 합집합
#include <unistd.h>

while
{
	if (dup[(unsigned char)argv[1][i]] == 0)
	{
		write
		dup[(unsigned char)argv[1][i]]++;
	}
	i++;
}
while
{
	if (dup[(unsigned char)argv[2][j]] == 0)
	{
		write
		dup[(unsigned char)argv[2][j]]++;
	}
	j++;
}
write(\n)


